# PromptDec Environment Configuration Template
# Copy this file to .env.local (web) or .env (backend) and fill in your values

# === WEB FRONTEND (Vite) ===
VITE_API_URL=http://localhost:8000
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
VITE_SENTRY_DSN=your-sentry-dsn-here

# === BACKEND (FastAPI) ===
DATABASE_URL=postgresql://user:password@localhost:5432/promptdec
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here

# GitHub OAuth
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Monitoring (optional)
SENTRY_DSN=your-backend-sentry-dsn

# CORS
CORS_ORIGINS=http://localhost:5173,https://promptdec.vercel.app

# === PRODUCTION OVERRIDES ===
# Production API URL
# VITE_API_URL=https://api.promptdec.com

# Production Database
# DATABASE_URL=postgresql://user:password@host:5432/promptdec_prod
